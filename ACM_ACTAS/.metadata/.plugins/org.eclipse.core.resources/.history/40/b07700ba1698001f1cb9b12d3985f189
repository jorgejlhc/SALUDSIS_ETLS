  <node componentName="tJavaRow" componentVersion="0.101" offsetLabelX="0" offsetLabelY="0" posX="2304" posY="96">
    <elementParameter field="TEXT" name="UNIQUE_NAME" value="tJavaRow_2" show="false"/>
    <elementParameter field="MEMO_JAVA" name="CODE" value="//Code generate according to input schema and output schema&#xD;&#xA;output_row.NUMERO_FACTURA = input_row.NUMERO_FACTURA;&#xD;&#xA;output_row.DOCUMENTO = input_row.DOCUMENTO;&#xD;&#xA;output_row.PACIENTE = input_row.PACIENTE;&#xD;&#xA;output_row.CODIGO = input_row.CODIGO;&#xD;&#xA;output_row.CONCEPTO_GLOSA = input_row.CONCEPTO_GLOSA;&#xD;&#xA;output_row.DESCRIPCION_GLOSA = input_row.DESCRIPCION_GLOSA;&#xD;&#xA;output_row.VALOR_GLOSADO = input_row.VALOR_GLOSADO;&#xD;&#xA;output_row.VALOR_COBRADO = input_row.VALOR_COBRADO;&#xD;&#xA;&#xA;context.CONTADOR ++ ;&#xA;&#xA;&#x9;if((context.FACTURA_ID.equals(input_row.FACTURA_ID)) &amp;&amp; (context.DOCUMENTO.equals(output_row.DOCUMENTO)))&#xA;&#x9;{&#xA;&#x9;&#x9;output_row.VALOR_SERVICIO = new BigDecimal(0);&#xA;&#x9;&#x9;context.SUMADOR_NETO = context.SUMADOR_NETO.subtract(output_row.VALOR_GLOSADO);&#xA;&#x9;&#x9;output_row.VALOR_NETO = context.SUMADOR_NETO;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;if( ((Integer)globalMap.get(&quot;tOracleInput_3_NB_LINE&quot;)) == context.CONTADOR )&#xA;&#x9;&#x9;{&#xA;&#x9;&#x9;&#x9;context.VNETO = context.VNETO.add(context.SUMADOR_NETO);&#xA;&#x9;&#x9;}&#xA;&#x9;}&#xA;&#xA;&#x9;else&#x9;&#xA;&#x9;{&#xA;&#x9;&#x9;context.VNETO = context.VNETO.add(context.SUMADOR_NETO);&#xA;&#x9;&#x9;output_row.VALOR_SERVICIO = input_row.VALOR_SERVICIO;&#xA;&#x9;&#x9;output_row.VALOR_NETO = output_row.VALOR_SERVICIO.subtract(output_row.VALOR_GLOSADO);&#xA;&#x9;&#xA;&#x9;&#x9;context.SUMADOR_NETO = &#x9;output_row.VALOR_NETO;&#xA;&#x9;}&#xA;&#xA;&#xA;context.FACTURA_ID = input_row.FACTURA_ID;&#xA;context.DOCUMENTO  = input_row.DOCUMENTO;&#xA;&#xA;// SE OBTIENE LOS TOTALES&#xA;context.VFACTURADO = context.VFACTURADO.add(output_row.VALOR_SERVICIO);&#xA;context.VGLOSADO   = context.VGLOSADO.add(output_row.VALOR_GLOSADO);&#xA;context.VCOBRADO   = context.VCOBRADO.add(output_row.VALOR_COBRADO);&#xA;//context.VNETO = context.VNETO.add(output_row.VALOR_NETO);"/>
    <elementParameter field="MEMO_IMPORT" name="IMPORT" value="//import java.util.List;"/>
