    <elementParameter field="GUESS_SCHEMA" name="GUESS_SCHEMA" value="&quot;&quot;"/>
    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;&#xD;&#xA;SELECT&#xD;&#xA;&#x9;    AF.ID AS FACTURA_ID,&#xD;&#xA;        AF.PREFIJO || AF.NUMERO AS NUMERO_FACTURA,&#xD;&#xA;        GT.SIGLA || ' ' || DS.DOCUMENTO AS DOCUMENTO,&#xD;&#xA;        DECODE(GP.ID, null, 'El usuario no se encuentra registrado en el sistema',  TRIM(GP.PRIMER_NOMBRE)||COALESCE(TRIM(GP.SEGUNDO_NOMBRE),' ')||TRIM(GP.PRIMER_APELLIDO)||TRIM(GP.SEGUNDO_APELLIDO)  ) AS PACIENTE,&#xD;&#xA;        ACG.CODIGO || ACE.CODIGO AS CODIGO,&#xD;&#xA;        AC.DESCRIPCION AS CONCEPTO_GLOSA,&#xD;&#xA;        HG.OBSERVACIONES AS DESCRIPCION_GLOSA,&#xD;&#xA;        SUM(DS.VTOTAL - DS.VMENORCOBRADO) AS VALOR_SERVICIO,&#xD;&#xA;        SUM(NVL(HG.VGLOSADO,0))   AS VALOR_GLOSADO,&#xD;&#xA;        SUM(NVL(DS.VMENORCOBRADO, 0)) AS VALOR_MCOBRADO&#xD;&#xA;    FROM&#xD;&#xA;        ACM_DFACTURASERVICIO DS&#xD;&#xA;        INNER JOIN ACM_FACTURA    &#x9;  AF   ON  AF.ID = DS.FACTURA_ID &#xD;&#xA;        LEFT  JOIN GEN_PERSONAS       GP   ON  GP.ID = DS.PERSONAS_ID&#xD;&#xA;        LEFT  JOIN GEN_TDOCUMENTOS    GT   ON  GT.ID = DS.TDOCUMENTOS_ID &#xD;&#xA;        LEFT  JOIN ACM_GLOSASCAUSADAS AG   ON  AG.RECEPCION_ID = DS.RECEPCION_ID AND AG.DFACTURASERVICIO_ID = DS.ID  AND AG.ESTADO &lt;> 'Eliminada' &#xD;&#xA;        LEFT  JOIN ACM_CAUSALGLOSAS   AC   ON  AC.ID =  AG.CAUSALGLOSAS_ID&#xD;&#xA;        LEFT  JOIN ACM_CONCEPTOS      ACG  ON  ACG.ID = AC.CONCEPTOGENERAL_ID      AND  ACG.TCONCEPTO = 'GENERAL'    AND  ACG.ESTADO = 'Activo'&#xD;&#xA;        LEFT  JOIN ACM_CONCEPTOS      ACE  ON  ACE.ID = AC.CONCEPTOESPECIFICO_ID  AND  ACE.TCONCEPTO = 'ESPECIFICO' AND  ACE.ESTADO = 'Activo'     &#xD;&#xA;        LEFT  JOIN ACM_HGLOSASCAUSADAS HG  ON  HG.GLOSASCAUSADAS_ID = AG.ID  AND  HG.HAUDITORIAS_ID =  '&quot; + context.HAUDITORIA_ID + &quot;'  AND  HG.ESTREGISTRO_ID = 1&#xD;&#xA;        LEFT  JOIN ACM_HAUDITORIAS     HA  ON  HA.ID = HG.HAUDITORIAS_ID     AND HA.TIPO =  'Etapa'           &#xD;&#xA;        LEFT JOIN ACM_ETAPASNORMATIVIDAD  EN   ON  EN.ID = HG.ETAPASNORMATIVIDAD_ID   AND EN.CODIGO = 1  AND EN.ESTREGISTRO_ID = 1     &#xD;&#xA;    WHERE  DS.RECEPCION_ID = '&quot; + context.RECEPCION_ID + &quot;'&#xD;&#xA;    AND AF.ESTREGISTRO_ID = 1 &#xD;&#xA;&#x9;AND AG.ID IS NULL&#xD;&#xA;    GROUP BY AF.ID, AF.PREFIJO || AF.NUMERO, GT.SIGLA || ' ' || DS.DOCUMENTO, DECODE(GP.ID, null, 'El usuario no se encuentra registrado en el sistema', TRIM(GP.PRIMER_NOMBRE)||COALESCE(TRIM(GP.SEGUNDO_NOMBRE),' ')||TRIM(GP.PRIMER_APELLIDO)||TRIM(GP.SEGUNDO_APELLIDO) ), ACG.CODIGO || ACE.CODIGO, &#xD;&#xA;    AC.DESCRIPCION, HG.OBSERVACIONES &#xD;&#xA;    &#xD;&#xA;    UNION ALL&#xD;&#xA;     SELECT&#xD;&#xA;&#x9;&#x9;AF.ID AS FACTURA_ID,&#xD;&#xA;        AF.PREFIJO || AF.NUMERO AS NUMERO_FACTURA,&#xD;&#xA;        DS.TDOCUMENTO || ' ' || DS.DOCUMENTO AS DOCUMENTO,&#xD;&#xA;        DECODE(GP.ID, null, 'El usuario no se encuentra registrado en el sistema',  TRIM(GP.PRIMER_NOMBRE)||COALESCE(TRIM(GP.SEGUNDO_NOMBRE),' ')||TRIM(GP.PRIMER_APELLIDO)||TRIM(GP.SEGUNDO_APELLIDO)  ) AS PACIENTE,&#xD;&#xA;        ACG.CODIGO || ACE.CODIGO AS CODIGO,&#xD;&#xA;        AC.DESCRIPCION AS CONCEPTO_GLOSA,&#xD;&#xA;        HG.OBSERVACIONES AS DESCRIPCION_GLOSA,&#xD;&#xA;        SUM(DS.VTOTAL - DS.VMENORCOBRADO) AS VALOR_SERVICIO,&#xD;&#xA;        SUM(NVL(HG.VGLOSADO,0))   AS VALOR_GLOSADO,&#xD;&#xA;        SUM(NVL(DS.VMENORCOBRADO, 0)) AS VALOR_MCOBRADO&#xD;&#xA;    FROM&#xD;&#xA;        ACM_DFACTURAFORMULACION DS&#xD;&#xA;        INNER JOIN ACM_FACTURA    &#x9;  AF   ON  AF.ID = DS.FACTURA_ID &#xD;&#xA;        LEFT JOIN GEN_PERSONAS       GP   ON  GP.DOCUMENTO = DS.DOCUMENTO AND GP.TDOCUMENTOS_ID = DS.TDOCUMENTOS_ID&#xD;&#xA;        LEFT  JOIN ACM_GLOSASCAUSADAS AG   ON  AG.RECEPCION_ID = DS.RECEPCION_ID AND AG.DFACTURAFORMULACION_ID = DS.ID  AND AG.ESTADO &lt;> 'Eliminada' &#xD;&#xA;        LEFT  JOIN ACM_CAUSALGLOSAS   AC   ON  AC.ID =  AG.CAUSALGLOSAS_ID&#xD;&#xA;        LEFT  JOIN ACM_CONCEPTOS      ACG  ON  ACG.ID = AC.CONCEPTOGENERAL_ID      AND  ACG.TCONCEPTO = 'GENERAL'    AND  ACG.ESTADO = 'Activo'&#xD;&#xA;        LEFT  JOIN ACM_CONCEPTOS      ACE  ON  ACE.ID = AC.CONCEPTOESPECIFICO_ID  AND  ACE.TCONCEPTO = 'ESPECIFICO' AND  ACE.ESTADO = 'Activo'     &#xD;&#xA;        LEFT  JOIN ACM_HGLOSASCAUSADAS HG  ON  HG.GLOSASCAUSADAS_ID = AG.ID  AND  HG.HAUDITORIAS_ID = '&quot; + context.HAUDITORIA_ID + &quot;'  AND  HG.ESTREGISTRO_ID = 1&#xD;&#xA;        LEFT  JOIN ACM_HAUDITORIAS     HA  ON  HA.ID = HG.HAUDITORIAS_ID     AND HA.TIPO =  'Etapa'           &#xD;&#xA;        LEFT JOIN ACM_ETAPASNORMATIVIDAD  EN   ON  EN.ID = HG.ETAPASNORMATIVIDAD_ID  AND EN.CODIGO = 1   AND EN.ESTREGISTRO_ID = 1     &#xD;&#xA;    WHERE  DS.RECEPCION_ID = '&quot; + context.RECEPCION_ID + &quot;'&#xD;&#xA;    AND AF.ESTREGISTRO_ID = 1&#xD;&#xA;&#x9;AND AG.ID IS NULL&#xD;&#xA;    GROUP BY AF.ID, AF.PREFIJO || AF.NUMERO, DS.TDOCUMENTO || ' ' || DS.DOCUMENTO, DECODE(GP.ID, null, 'El usuario no se encuentra registrado en el sistema', TRIM(GP.PRIMER_NOMBRE)||COALESCE(TRIM(GP.SEGUNDO_NOMBRE),' ')||TRIM(GP.PRIMER_APELLIDO)||TRIM(GP.SEGUNDO_APELLIDO) ), ACG.CODIGO || ACE.CODIGO, &#xD;&#xA;    AC.DESCRIPCION, HG.OBSERVACIONES &#xD;&#xA;    ORDER BY NUMERO_FACTURA, DOCUMENTO &quot;"/>
    <elementParameter field="LABEL" name="NOTE" value="This option only applies when deploying and running in the Talend Runtime" show="false"/>
