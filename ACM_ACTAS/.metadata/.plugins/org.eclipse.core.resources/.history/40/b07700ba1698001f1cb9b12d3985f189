    <elementParameter field="GUESS_SCHEMA" name="GUESS_SCHEMA" value="&quot;&quot;"/>
    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;SELECT  &#xD;&#xA;&#x9;&#x9;AF.ID AS FACTURA_ID,&#xD;&#xA;&#x9; &#x9;NVL(AF.PREFIJO,' ') || AF.NUMERO AS NUMERO_FACTURA,  &#xD;&#xA;        GT.SIGLA || ' ' || DS.DOCUMENTO AS DOCUMENTO,&#xD;&#xA;        TRIM(GP.PRIMER_NOMBRE)||COALESCE(TRIM(GP.SEGUNDO_NOMBRE),' ')||TRIM(GP.PRIMER_APELLIDO)||TRIM(GP.SEGUNDO_APELLIDO) AS PACIENTE,&#xD;&#xA;        ACG.CODIGO || ACE.CODIGO AS CODIGO,&#xD;&#xA;        ACE.NOMBRE AS CONCEPTO_GLOSA,&#xD;&#xA;        AG.OBSERVACION AS DESCRIPCION_GLOSA,&#xD;&#xA;        NVL(HN.OBSERVACIONES, 'No registra') AS RESPUESTA, &#xD;&#xA;        NVL(HG.OBSERVACIONES, 'No registra') AS REVISION_RESPUESTA,&#xD;&#xA;        HG.VGLOSADO AS VALOR_GLOSADO,&#xD;&#xA;        HG.VACEPTADO AS VALOR_ACEPTADO,&#xD;&#xA;        HG.VSOPORTADO AS VALOR_SOPORTADO,&#xD;&#xA;        HG.VRATIFICADO AS VALOR_RATIFICADO,&#xD;&#xA;        HG.VSOPORTADO  AS VNETO     &#xD;&#xA;    FROM&#xD;&#xA;        ACM_DFACTURAFORMULACION DS&#xD;&#xA;        INNER JOIN ACM_FACTURA    &#x9;  AF   ON  AF.ID = DS.FACTURA_ID &#xD;&#xA;        INNER JOIN GEN_PERSONAS       GP   ON  GP.DOCUMENTO = DS.DOCUMENTO AND GP.TDOCUMENTOS_ID = DS.TDOCUMENTOS_ID&#xD;&#xA;        INNER JOIN GEN_TDOCUMENTOS    GT   ON  GT.ID = DS.TDOCUMENTOS_ID&#xD;&#xA;        INNER JOIN ACM_GLOSASCAUSADAS AG   ON  AG.RECEPCION_ID = DS.RECEPCION_ID AND AG.DFACTURAFORMULACION_ID = DS.ID&#xD;&#xA;        INNER JOIN ACM_CAUSALGLOSAS   AC   ON  AC.ID =  AG.CAUSALGLOSAS_ID&#xD;&#xA;        INNER JOIN ACM_CONCEPTOS      ACG  ON  ACG.ID = AC.CONCEPTOGENERAL_ID     AND  ACG.TCONCEPTO = 'GENERAL'    AND  ACG.ESTADO = 'Activo'&#xD;&#xA;        INNER JOIN ACM_CONCEPTOS      ACE  ON  ACE.ID = AC.CONCEPTOESPECIFICO_ID  AND  ACE.TCONCEPTO = 'ESPECIFICO' AND  ACE.ESTADO = 'Activo'     &#xD;&#xA;        INNER JOIN ACM_HGLOSASCAUSADAS HG  ON  HG.GLOSASCAUSADAS_ID = AG.ID   &#xD;&#xA;        INNER JOIN ACM_HAUDITORIAS     HA  ON  HA.ID = HG.HAUDITORIAS_ID              &#xD;&#xA;        INNER JOIN ACM_ETAPASNORMATIVIDAD  EN   ON  EN.ID = HG.ETAPASNORMATIVIDAD_ID   AND EN.ESTREGISTRO_ID = 1 &#xD;&#xA;        INNER JOIN  &#xD;&#xA;        (&#xD;&#xA;            SELECT HGL.OBSERVACIONES, AGS.DFACTURAFORMULACION_ID, ETP.CODIGO,  AGS.ID&#xD;&#xA;            FROM ACM_GLOSASCAUSADAS AGS&#xD;&#xA;            INNER JOIN ACM_HGLOSASCAUSADAS    HGL ON HGL.GLOSASCAUSADAS_ID = AGS.ID&#xD;&#xA;            INNER JOIN ACM_ETAPASNORMATIVIDAD ETP ON ETP.ID = HGL.ETAPASNORMATIVIDAD_ID &#xD;&#xA;            WHERE AGS.RECEPCION_ID = '&quot; + context.RECEPCION_ID + &quot;' &#xD;&#xA;            AND AGS.ESTREGISTRO_ID = 1&#xD;&#xA;            AND HGL.ESTREGISTRO_ID = 1&#xD;&#xA;            AND HGL.ESTADO = 'Revisada'          &#xD;&#xA;&#xD;&#xA;        )  HN ON HN.ID = AG.ID AND HN.DFACTURAFORMULACION_ID = DS.ID  AND HN.CODIGO = (EN.CODIGO - 1)&#xD;&#xA;    WHERE  DS.RECEPCION_ID =  '&quot; + context.RECEPCION_ID + &quot;' &#xD;&#xA;    AND  HG.HAUDITORIAS_ID =  '&quot; + context.HAUDITORIA_ID + &quot;'&#xD;&#xA;    AND  HG.ESTADO = 'Revisada'&#xD;&#xA;    AND  HG.ESTREGISTRO_ID = 1&#xD;&#xA;    AND AF.ESTREGISTRO_ID = 1&#xD;&#xA;&#x9;ORDER BY NUMERO_FACTURA, DOCUMENTO &quot;"/>
    <elementParameter field="LABEL" name="NOTE" value="This option only applies when deploying and running in the Talend Runtime" show="false"/>
    <elementParameter field="CHECK" name="SPECIFY_DATASOURCE_ALIAS" value="false" show="false"/>
    <elementParameter field="TEXT" name="DATASOURCE_ALIAS" value="&quot;&quot;" show="false"/>
    <elementParameter field="MAPPING_TYPE" name="MAPPING" value="oracle_id" show="false"/>
